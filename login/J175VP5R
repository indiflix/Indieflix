<script src="/js/codehim.js?v2.8"></script>
<script>
(function($){
//Readmore text in code overview			
  $("#readMoreToggle").click(function() {
    var elem = $(this).text();
    if (elem == "Read More") {
      //Stuff to do when btn is in the read more state
      $(this).text("Read Less");
      $("#moretext").slideDown(300);
    } else {
      //Stuff to do when btn is in the read less state
      $(this).text("Read More");
      $("#moretext").slideUp(300);
    }
  });

	//Add shadow class when navbar sticky
	$(window).on('scroll', function(){
		
		if ($('.nav-fixed').offset().top > 101){
            $('.nav-fixed').addClass('nav_shadow');
        }else{
            $('.nav-fixed').removeClass('nav_shadow');
        }
	});
})(jQuery);
	</script>
</body>
</html>